.character {
    width: 64px;
    height: 64px;
    /* background-color: red; */
}

.center {
	position: absolute;
    top:0;
    bottom: 0;
    left: 0;
    right: 0;
    
    margin: auto;
}

.character > img {
	position:relative;
	width: 64px;
    height: 64px;
    image-rendering: crisp-edges;
}